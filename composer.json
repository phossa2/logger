{
    "name": "phossa2/logger",
    "license": "MIT",
    "type": "library",
    "description": "A PSR-3 compliant logging libraray for PHP",
    "keywords": ["psr", "psr-3", "logger", "logging", "phossa"],
    "homepage": "https://github.com/phossa2/logger",
    "authors": [
        {
            "name": "Hong Zhang",
            "homepage": "https://github.com/phossa2"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "psr/log": "^1.0",
        "phossa2/shared": "^2.0.21"
    },
    "autoload": {
        "psr-0": { "Phossa2\\Logger\\": "src/" }
    },
    "require-dev": {
        "phpunit/phpunit": "4.*"
    },
    "provide": {
        "psr/log-implementation": "1.0.0"
    },
    "replace": {
        "phossa/phossa-logger": "*"
    },
    "config": {
        "platform": {
            "php": "5.4.0"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    },
    "archive": {
        "exclude": [ "!vendor", "tests", "composer.lock" ]
    }
}
