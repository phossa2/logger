{
    "name": "phossa2/logger",
    "license": "MIT",
    "type": "library",
    "description": "A PSR-3 compliant logging libraray for PHP",
    "keywords": ["psr", "psr-3", "logger", "logging", "phossa", "phossa2"],
    "homepage": "https://github.com/phossa2/logger",
    "authors": [
        {
            "name": "Hong Zhang",
            "homepage": "https://github.com/phossa2"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "psr/log": "^1.0",
        "phossa2/shared": "^2.0.21"
    },
    "autoload": {
        "psr-4": { "Phossa2\\Logger\\": "Logger/" }
    },
    "require-dev": {
        "phpunit/phpunit": "4.*",
        "squizlabs/php_codesniffer": "2.*"
    },
    "provide": {
        "psr/log-implementation": "1.0.0"
    },
    "scripts": {
        "test": "phpunit",
        "check": "phpcs --standard=psr2 Logger/",
        "format": "phpcbf --standard=psr2 Logger/"
    },
    "config": {
        "platform": {
            "php": "5.4.0"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.1-dev"
        }
    },
    "archive": {
        "exclude": [ "!vendor", "tests", "composer.lock" ]
    }
}
